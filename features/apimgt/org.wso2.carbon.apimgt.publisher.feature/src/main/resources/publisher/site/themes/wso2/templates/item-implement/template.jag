																			<%if(corsJson != null && corsJson.corsConfigurationEnabled){%> checked="checked"<%}%>>
																						<span class="helper"><%=i18n.localize("corsMsg")%></span>
																	</label>
																</div>
															</div>
															   <div class="form-group form-inline">
																<div class="col-sm-3"></div>
																<div class="col-sm-9">
