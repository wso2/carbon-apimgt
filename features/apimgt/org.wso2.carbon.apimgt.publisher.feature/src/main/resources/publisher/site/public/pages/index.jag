<html lang="en">
<%
    var app = require("/site/public/conf/settings.js").AppConfig.app;
%>

<head>
    <base href="<%= app.context %>/" />
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=1024, initial-scale=1"/>
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>[Publisher]WSO2 APIM</title>

    <link href="<%= app.context %>/site/public/css/main.css" type="text/css" rel="stylesheet" />
    <link href="<%= app.context %>/site/public/css/draftjs.css" type="text/css" rel="stylesheet" />
    <link rel="shortcut icon" href="<%= app.context %>/site/public/images/favicon.ico">
    <link rel="icon" type="image/x-icon" href="<%= app.context %>/site/public/images/_favicon.ico">
</head>

<body>

    <div id="react-root">
        <div class="apim-dual-ring"></div>
    </div>

    <script src="<%= app.context %>/site/public/fonts/iconfont/MaterialIcons.js"></script>
    <script src="<%= app.context %>/site/public/conf/defaultTheme.js"></script>
    <script src="<%= app.context %>/site/public/conf/settings.js"></script>
    <script src="<%= app.context %>/site/public/dist/index.bundle.js"></script>
    <link rel="stylesheet" href="<%= app.context %>/site/public/fonts/iconfont/material-icons.css">
</body>

</html>
