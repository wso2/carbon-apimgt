        "rc-notification": "^3.0.1",
        "rc-progress": "^2.6.1",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.13.1",
        "react-dropzone": "^10.2.2",
