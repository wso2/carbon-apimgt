                                <bundleDef>com.github.ben-manes.caffeine:caffeine:${caffeine.version}</bundleDef>
                                <bundleDef>commons-codec:commons-codec</bundleDef>
                                <bundleDef>
                                    org.wso2.carbon.mediation:org.wso2.carbon.sequences.stub:${carbon.mediation.version}
                                </bundleDef>
                                <bundleDef>
                                    org.wso2.carbon.mediation:org.wso2.carbon.endpoint.stub:${carbon.mediation.version}
                                </bundleDef>
                                <importBundleDef>
                                    org.wso2.carbon.mediation:org.wso2.carbon.rest.api.stub:${carbon.mediation.version}
                                </importBundleDef>
                                <importBundleDef>
                                    org.wso2.carbon.mediation:org.wso2.carbon.localentry.stub:${carbon.mediation.version}
