    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy</id>
                        <phase>package</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>org.wso2.carbon.apimgt</groupId>
                                    <artifactId>org.wso2.carbon.apimgt.samples.calculator</artifactId>
                                    <overWrite>true</overWrite>
                                    <type>war</type>
                                    <outputDirectory>${basedir}/src/main/resources/</outputDirectory>
                                    <destFileName>am#sample#calculator#v1.war</destFileName>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>org.wso2.carbon.apimgt</groupId>
                                    <artifactId>org.wso2.carbon.apimgt.samples.pizzashack</artifactId>
                                    <overWrite>true</overWrite>
                                    <type>war</type>
                                    <outputDirectory>${basedir}/src/main/resources/</outputDirectory>
                                    <destFileName>am#sample#pizzashack#v1.war</destFileName>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

