<%
var updateApplication = function (applicationNew,applicationOld,username,tier,callbackUrlNew,descriptionNew) {
    var log = new Log(),
            store = jagg.module("manager").getAPIStoreObj();

    try {
        var groupIds = session.get("groupIds");
        store.updateApplication(applicationNew,applicationOld,username,tier,callbackUrlNew,descriptionNew,groupIds);
        if (log.isDebugEnabled()) {
            log.debug("updateApplication");
        }
        return {
            error:false
        };
    } catch (e) {
        log.error(e.message);
        return {
            error:e
        };
    }
};
%>