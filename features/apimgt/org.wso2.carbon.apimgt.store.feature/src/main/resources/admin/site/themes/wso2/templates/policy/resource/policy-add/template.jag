<% jagg.template("policy/resource/policy-add", function(inputs, outputs, jagg) { %>
<%
    var log = new Log();
    var policyName = request.getParameter("policyName");
    policyName = policyName == null ? "" : policyName;
    var pageTitle = i18n.localize("Add Advanced Throttle Policy");
    var nameFieldDisableStatus = "";
    var policyAction = encode.forJavaScript(request.getParameter("action"));
    var policyDesc = "",policyLevel = "", defaultQuotaPolicy = "", defaultBandwidth = "" , defaultBandwidthUnit = "",
                        defaultRequestCount = "", defaultUnitTime="", defaultTimeUnit="";
    var APIUtil = org.wso2.carbon.apimgt.impl.utils.APIUtil;
    var queryParamPublish = APIUtil.isQueryParamDataPublishingEnabled();
    var headerPublish = APIUtil.isHeaderDataPublishingEnabled();
    var jwtPublish = APIUtil.isJwtTokenPublishingEnabled();
    var action = "new";
    if(policyName != null && policyName != ""){
        action = "edit";
        pageTitle = i18n.localize("Edit Advanced Throttle Policy");
        nameFieldDisableStatus = "readonly";
        var mod = jagg.module("policy");
        var apiPolicy = mod.getAPIPolicy(policyName).apiPolicy;
        policyName = apiPolicy.getPolicyName() == null ? "" : apiPolicy.getPolicyName();
        policyDesc = apiPolicy.getDescription() == null ? "" : apiPolicy.getDescription();
        policyLevel = apiPolicy.getUserLevel() == null ? "" : apiPolicy.getUserLevel();
        defaultQuotaPolicy = apiPolicy.getDefaultQuotaPolicy().getType();
        if (defaultQuotaPolicy == "requestCount"){
            defaultRequestCount = apiPolicy.getDefaultQuotaPolicy().getLimit().getRequestCount();
        }
        else if (defaultQuotaPolicy == "bandwidthVolume"){
            defaultBandwidth = apiPolicy.getDefaultQuotaPolicy().getLimit().getDataAmount();
            defaultBandwidthUnit = apiPolicy.getDefaultQuotaPolicy().getLimit().getDataUnit();
        }

        defaultUnitTime = apiPolicy.getDefaultQuotaPolicy().getLimit().getUnitTime();
        defaultTimeUnit = apiPolicy.getDefaultQuotaPolicy().getLimit().getTimeUnit();
    }
    %>

<div id="throttle-policies">

	<div class="page-header">
        <h2 id="tierTopic"><%=pageTitle%></h2>
        <p class="lead"></p>
    </div>

        <form class="form-horizontal" method="" id="" action="">
	        
	        <div class="page-header">
	            <h2><br></h2>
	            <p class="lead"><%=i18n.localize("General Details")%></p>
	        </div>
	        
	        <input type="hidden" id="action" name="action" value="<%=action%>">
       <input type='hidden' id='errorMsgRequired' name='errorMsgRequired'
                value='<%=i18n.localize("Required")%>'/>
                <input type='hidden' id='errorMessageInvalid' name='errorMessageInvalid'
                value='<%=i18n.localize("Invalid")%>'/>
                <input type='hidden' id='errorMessageIllegalChar' name='errorMessageIllegalChar'
                value='<%=i18n.localize("Name contains one or more illegal characters ")%>'/>
                <input type='hidden' id='errorMessageSpaces' name='errorMessageSpaces'
                value='<%=i18n.localize("Name contains spaces")%>'/>

       		<div class="form-group">
	            <label class="control-label col-sm-3"><%=i18n.localize("Name")%>: <span class="requiredAstrix">*</span>
	            		<a class="help_popup" help_data="name_help" data-trigger="hover" data-placement="bottom" title="">
							<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>
						</a>
                    	<p id="name_help" class="hide"><%=i18n.localize("Name of the throttle policy.")%></p>
	            
	            </label>
	            <div class="col-md-9">
	                <input class="form-control required validRegistryName validateForwardSlashAtEnd validInput apiNameExists noSpace" type="text" id="policy-name" name="policy-nam" autofocus="autofocus" value="<%=encode.forHtml(String(policyName))%>" <%=nameFieldDisableStatus%>/>
	            </div>
        	</div>
            <div class="form-group">
                <label class="control-label col-sm-3" for="policy-description"><%=i18n.localize("Description")%>:
                    <a class="help_popup" help_data="desc_help" data-trigger="hover" data-placement="bottom" data-original-title="" title="">
                    	<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>                    
                    </a>
                    <p id="desc_help" class="hide"><%=i18n.localize("Description of the throttle policy.")%></p>
                </label>
                <div class="col-sm-9">
                	<textarea class="form-control validInput" id="policy-description" cols="40" rows="2" name="policy-description"><%=encode.forHtml(String(policyDesc))%></textarea>
                </div>
            </div>            
            <!--<div class="form-group">
                <label class="control-label col-sm-3" for="level">Level:
                	 <a class="help_popup" help_data="level_help" data-trigger="hover" data-placement="bottom" data-original-title="" title="">
                    	<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>                    
                    </a>
                    <p id="level_help" class="hide"><strong>API Level :</strong><%=i18n.localize("Will be applicable to the whole API.")%> <br><strong> User Level :</strong>  <%=i18n.localize("Applicable in the user level.")%><br></p>
                </label>
                <div class="col-sm-9">
                    <select class="form-control select required valid" id="policy-level" name="level">
                        <option value="apiLevel" <% if(policyLevel == "" || policyLevel == "apiLevel") {%>selected<%}%> >API</option>
                        <option value="userLevel" <% if(policyLevel == "userLevel") {%>selected<%}%> >User</option>
                    </select>
                </div>
            </div>-->

            <div class="page-header">
                <h2><br/></h2>
                <p class="lead"><%=i18n.localize("Default Limits")%></p>
            </div>

            <div id="quotaTypeRadioDev">
            <div class="form-group">
                <label class="control-label col-sm-3"></label>
                <div class="radio">
                    <label class="checkbox-inline">
                        <input type="radio"  name="select-quota-type" value="requestCount" id="requestCountType" <%if(defaultQuotaPolicy == "" || defaultQuotaPolicy == "requestCount"){%> checked <%}%> onchange="showHideDefaultQuotaPolicy()"/>
                        <%=i18n.localize("Request Count")%>
                    </label>
                </div>
                <div class="radio">
                    <label class="checkbox-inline">
                        <input type="radio" name="select-quota-type" value="bandwidthVolume" id="bandwidthType" <%if(defaultQuotaPolicy == "bandwidthVolume"){%> checked <%}%> onchange="showHideDefaultQuotaPolicy()"/>
                        <%=i18n.localize("Request Bandwidth")%>
                    </label>
                </div>
            </div>
        </div>
        
        <div id="defaultRequestCountBasedDiv">
            <div class="form-group">
                <label class="control-label col-sm-3">Request Count: <span class="requiredAstrix">*</span>
                	<a class="help_popup" help_data="request_count_help" data-trigger="hover" data-placement="bottom" data-original-title="" title="">
                    	<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>                    
                    </a>
                    <p id="request_count_help" class="hide"><%=i18n.localize("Number of requests allowed.")%></p>
                 </label>
                <div class="col-sm-9">
                    <input class="form-control required validInput noSpace" type="text" id="request-count" name="request-count" autofocus="autofocus" value="<%=encode.forHtml(String(defaultRequestCount))%>" />
                </div>
            </div>
        </div>
        <div id="defaultBandwidthBasedDiv">
            <div class="form-group">
                <label class="control-label col-sm-3">Data Bandwidth: <span class="requiredAstrix">*</span>
                	<a class="help_popup" help_data="bandwidth_help" data-trigger="hover" data-placement="bottom" data-original-title="" title="">
                    	<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>                    
                    </a>
                    <p id="bandwidth_help" class="hide">Bandwidth Allowed.</p>
                </label>
                <div class="col-sm-6">
                   <input class="form-control required validInput noSpace" type="text" id="bandwidth" name="bandwidth" autofocus="autofocus" value="<%=encode.forHtml(String(defaultBandwidth))%>" />
                </div>
                <div class="col-sm-3">
                    <select class="form-control select required" id="bandwidth-unit" name="bandwidth-unit">
                        <option value="KB"  selected="true">KB</option>
                        <option value="MB">MB</option>
                    </select>
                </div>
            </div>
        </div>
        
         <div class="form-group">
            <label class="control-label col-sm-3">Unit Time: <span class="requiredAstrix">*</span>
            	<a class="help_popup" help_data="unit_time_help" data-trigger="hover" data-placement="bottom" data-original-title="" title="">
                    	<span class="icon fw-stack fw-lg" style="font-size:6px">
								<i class="fw fw-circle fw-stack-2x" data-original-title="" title=""></i>
								<i class="fw fw-question fw-stack-1x fw-inverse" style="font-size:8px" data-original-title="" title=""></i>
							</span>                    
                    </a>
                    <p id="unit_time_help" class="hide">Time Configuration.</p>
            </label>
            <div class="col-sm-6">
                <input class="form-control" type="text" id="unit-time-count" name="unit-time-count" autofocus="autofocus" value="<%=encode.forHtml(String(defaultUnitTime))%>" />
            </div>
            <div class="col-sm-3">
                <select class="form-control select required" id="request-count-unit" name="request-count-unit">
                    <option value="min" <% if(defaultTimeUnit == "" || defaultTimeUnit == "min") {%>selected<%}%>>Minutes(s)</option>
                    <option value="sec" <% if(defaultTimeUnit == "sec") {%>selected<%}%>>Second(s)</option>
                    <option value="hour" <% if(defaultTimeUnit == "hour") {%>selected<%}%>>Hour(s)</option>
                    <option value="days" <% if(defaultTimeUnit == "day") {%>selected<%}%>>Day(s)</option>
                    <option value="month" <% if(defaultTimeUnit == "month") {%>selected<%}%>>Month(s)</option>
                    <option value="year" <% if(defaultTimeUnit == "year") {%>selected<%}%>>Year(s)</option>
                </select>
            </div>
        </div>
 </form>


    <div class="page-header">
        <h2><br/></h2>
        <p class="lead"><%=i18n.localize("Conditional Groups")%></p>
    </div>
    <div id="throttle-policies-pipeline-content" class="form-group">
        <button class="btn btn-secondary" type="button" onclick="addPolicy()">
            <span class="icon fw-stack">
                <i class="fw fw-add fw-stack-1x"></i>
                <i class="fw fw-circle-outline fw-stack-2x"></i>
                </span>&nbsp;<%=i18n.localize("Add Conditional Group")%>
        </button>
    </div>
    <div id="pipeline-content" class="form-group"></div>
    <div class="page-header">
        <h2><br/></h2>
    </div>
    <% if(!queryParamPublish || !headerPublish || !jwtPublish) { %>
        <div class="alert alert-warning" role="alert">
            <i class="icon fw fw-info"></i>
            <strong><%=i18n.localize("Warning!")%></strong>
            <%=i18n.localize("Publishing")%>
            <% if(!queryParamPublish) { %> <%=i18n.localize("Query Params") %>
                <% if(!headerPublish && !jwtPublish){ %>
                    <%=i18n.localize(",")%> <%}
                else if (!headerPublish || !jwtPublish) { %>
                    <%=i18n.localize("and")%> <%}}
            if(!headerPublish){%><%=i18n.localize("Header Data")%> <%
                if(!jwtPublish){%>
                <%=i18n.localize("and")%><%=i18n.localize("JWT Token")%> <% }}
            else{%><%=i18n.localize("JWT Token")%> <%}%>
            <%=i18n.localize("isn't configured. If a policy configured with any of these conditions, it won't be applied.")%>
            <button type="button" class="close" aria-label="close" data-dismiss="alert">
                <span aria-hidden="true">
                    <i class="fw fw-cancel"></i>
                </span>
            </button>
        </div>
    <% } %>
    <div id="add-policy-button" class="form-group">
        <button type="button" id="addThrottleBtn" name="addThrottleBtn" onclick="addPolicyToBackend()" class="btn btn-primary has-spinner">
            <span class="icon fw-stack">
                <i class="fw fw-save fw-stack-1x"></i>
                <i class="fw fw-circle-outline fw-stack-2x"></i>
            </span>
            <% if (action == "new") {%>
             &nbsp;<%=i18n.localize("Save")%>
             <% } else { %>
            &nbsp;<%=i18n.localize("Update")%>
            <% } %>
        </button>    
        <button type="button" id="cancel-tier-btn" class="btn btn-secondary">
            <span class="icon fw-stack">
                <i class="fw fw-cancel fw-stack-1x"></i>
                <i class="fw fw-circle-outline fw-stack-2x"></i>
            </span>
            <%=i18n.localize("Cancel")%>
        </button>   
    </div>
</div>

<script id="designer-policy-template" type="text/x-handlebars-template">
<div class="panel-group form-group accordion" role="tablist" aria-multiselectable="true">
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="collapse-{{executionFlow.id}}">
            <h4 class="panel-title">
                <label class="radio" data-toggle="collapse" href="#pipeline-{{executionFlow.id}}" aria-expanded="false" aria-controls="pipeline-{{executionFlow.id}}">
                    <span class="fw-stack fw-lg">
                        <i class="fw fw-circle fw-stack-2x fw-inverse"></i>
                        <i class="fw fw-number fw-stack-1x">{{executionFlow.id}}</i>
                    </span>
                </label>
                <div>
                    <div class="heading" data-toggle="collapse" href="#pipeline-{{executionFlow.id}}" aria-expanded="false" aria-controls="pipeline-{{executionFlow.id}}"><%=i18n.localize("Condition Group")%></div>
                    <div class="description truncate title" title="">
                        <a href="#" id="executionFlow-desc-{{executionFlow.id}}" data-type="text" data-pk="1" data-title="<%=i18n.localize("Enter Description")%>">{{executionFlow.description}}</a>
                    </div>
                </div>
                <div class="clearfix"></div>
            </h4>
        </div>
        <div id="pipeline-{{executionFlow.id}}" class="panel-collapse collapse" role="tabpanel" aria-labelledby="collapse-{{executionFlow.id}}">
            <div class="panel-body">
                <div class="row">
                    <div class='wr-advance-operations'>
                        <div class="wr-hidden-operations-nav col-lg-4">
                            <a href="javascript:void(0)" onclick="showAdvanceOperation('ip-condition-{{executionFlow.id}}', this, '{{executionFlow.id}}')" class="selected">
                                <span class="wr-hidden-operations-icon fw-stack">
                                    <i class="fw fw-key fw-stack-2x"></i>
                                </span>
                                IP Condition
                                {{#each executionFlow.conditions}}
                                    {{#if_eq type "IP"}}
                                        {{#if ../enabled}}
                                            <span id="ip-condition-configured-{{../../../executionFlow.id}}" class="has-success status-icon"><i id="ip-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-check"></i></span>
                                        {{else}}
                                            <span id="ip-condition-configured-{{../../../executionFlow.id}}" class="has-configured status-icon"><i id="ip-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-circle-outline"></i></span>
                                        {{/if}}
                                    {{/if_eq}}
                                {{/each}}
                            </a>
                            <a href="javascript:void(0)" onclick="showAdvanceOperation('header-condition-{{executionFlow.id}}', this, '{{executionFlow.id}}')">
                                <span class="wr-hidden-operations-icon fw-stack">
                                    <i class="fw fw-key fw-stack-2x"></i>
                                </span>
                                Header Condition
                                {{#each executionFlow.conditions}}
                                    {{#if_eq type "Header"}}
                                        {{#if ../enabled}}
                                            <span id="header-condition-configured-{{../../../executionFlow.id}}" class="has-success status-icon"><i id="header-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-check"></i></span>
                                        {{else}}
                                            <span id="header-condition-configured-{{../../../executionFlow.id}}" class="has-configured status-icon"><i id="header-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-circle-outline"></i></span>
                                        {{/if}}
                                    {{/if_eq}}
                                {{/each}}
                            </a>
                            <a href="javascript:void(0)" onclick="showAdvanceOperation('query-param-condition-{{executionFlow.id}}', this, '{{executionFlow.id}}')">
                                <span class="wr-hidden-operations-icon fw-stack">
                                    <i class="fw fw-key fw-stack-2x"></i>
                                </span>
                                Query Param Condition
                                {{#each executionFlow.conditions}}
                                    {{#if_eq type "QueryParam"}}
                                        {{#if ../enabled}}
                                            <span id="queryparam-condition-configured-{{../../../executionFlow.id}}" class="has-success status-icon"><i id="queryparam-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-check"></i></span>
                                        {{else}}
                                            <span id="queryparam-condition-configured-{{../../../executionFlow.id}}" class="has-configured status-icon"><i id="queryparam-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-circle-outline"></i></span>
                                        {{/if}}
                                    {{/if_eq}}
                                {{/each}}
                            </a>
                            <a href="javascript:void(0)" onclick="showAdvanceOperation('jwt-claim-condition-{{executionFlow.id}}', this, '{{executionFlow.id}}')">
                                <span class="wr-hidden-operations-icon fw-stack">
                                    <i class="fw fw-key fw-stack-2x"></i>
                                </span>
                                JWT Claim Condition
                                {{#each executionFlow.conditions}}
                                    {{#if_eq type "JWTClaim"}}
                                        {{#if ../enabled}}
                                            <span id="claim-condition-configured-{{../../../executionFlow.id}}" class="has-success status-icon"><i id="claim-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-check"></i></span>
                                        {{else}}
                                            <span id="claim-condition-configured-{{../../../executionFlow.id}}" class="has-configured status-icon"><i id="claim-condition-configured-icon-{{../../../executionFlow.id}}" class="fw fw-circle-outline"></i></span>
                                        {{/if}}
                                    {{/if_eq}}
                                {{/each}}
                            </a>
                        </div>
                        <div class="wr-hidden-operations-content col-lg-8">
                            <!-- IP Condition policy -->
                            {{#each executionFlow.conditions}}
                            {{#if_eq type "IP"}}
                            <div class="wr-hidden-operation" data-operation="ip-condition-{{../../executionFlow.id}}" style="display: block">
                                <div class="panel panel-default operation-data" data-operation="ip-condition-policy" data-operation-code="IP_CONDITION_POLICY">
                                    <div id="ip-condition-heading-{{../../executionFlow.id}}" class="panel-heading" role="tab">
                                        <h2 class="sub-title panel-title">
                                            <%=i18n.localize("IP Condition Policy")%>
                                            <label id="ip-condition-lbl-{{../../executionFlow.id}}" class="wr-input-control switch" data-toggle="collapse" data-target="#ip-condition-body-{{../../executionFlow.id}}">
                                                <input id="ip-condition-checkbox-{{../../executionFlow.id}}" onclick="changeStatusIcon({{../../executionFlow.id}}, 'IP', this)" {{#if ../enabled}}checked{{/if}} type="checkbox" />
                                                <span class="helper"></span>
                                                <span class="text"></span>
                                            </label>
                                            <hr>
                                            <div class="panel-title-description">
                                               <%=i18n.localize("This configuration is used to throttle by IP address.")%>
                                            </div>
                                        </h2>
                                    </div>
                                    <div id="ip-condition-body-{{../../executionFlow.id}}" class="panel-collapse panel-body collapse {{#if ../enabled}}in{{/if}}" role="tabpanel" aria-labelledby="ip-condition-body-{{../../executionFlow.id}}">
                                        <div id="ip-condition-feature-error-msg-{{../../executionFlow.id}}" class="alert alert-danger hidden" role="alert">
                                            <i class="icon fw fw-error"></i><span></span>
                                        </div>

                                        <div class="form-horizontal">
                                            <div class="form-group">
                                                <label class="control-label col-sm-3" for="ip-condition-type"><%=i18n.localize("IP Condition Type:")%><span class="requiredAstrix">*</span></label>
                                                <div class="col-sm-9">
                                                    <select  onchange="onIpConditionChange('{{../../executionFlow.id}}', this);" class="form-control select required valid" id="ip-condition-type-{{../../executionFlow.id}}" name="ip-condition-type-{{../../executionFlow.id}}">
                                                        {{#if_eq ../ipType "specific"}}
                                                            <option value="specificIp" selected><%=i18n.localize("Specific IP")%></option>
                                                            <option value="IPRange"><%=i18n.localize("IP Range")%></option>
                                                        {{else}}
                                                            <option value="specificIp"><%=i18n.localize("Specific IP")%></option>
                                                            <option value="IPRange" selected><%=i18n.localize("IP Range")%></option>
                                                        {{/if_eq}}
                                                    </select>
                                                </div>
                                            </div>
                                            {{#if_eq ../ipType "specific"}}
                                                <div class="form-group" style="display:block" id="specific-ip-block-{{../../../executionFlow.id}}">
                                                    <label class="control-label col-sm-3" for="request-count"><%=i18n.localize("IP Address:")%><span class="requiredAstrix">*</span></label>
                                                    <div class="col-sm-9">
                                                        <input type="text" class="form-control required validInput" id="specific-ip-address-input-{{../../../executionFlow.id}}" name="specific-ip-address-input-{{../../../executionFlow.id}}" autofocus="autofocus" value="{{../../specificIP}}" onchange="onIPChange(this);">
                                                    </div>
                                                </div>

                                                <div class="form-group" style="display:none" id="ip-range-block-{{../../../executionFlow.id}}">
                                                    <label class="control-label col-sm-3" for="request-count"><%=i18n.localize("Start IP Address:")%><span class="requiredAstrix">*</span></label>
                                                    <div class="col-sm-3">
                                                        <input type="text" class="form-control required validInput" id="ip-range-start-address-input-{{../../../executionFlow.id}}" name="ip-range-start-address-input-{{../../../executionFlow.id}}" autofocus="autofocus" value="" onchange="onIPChange(this);">
                                                    </div>

                                                    <label class="control-label col-sm-3" for="request-count"><%=i18n.localize("End IP Address:")%><span class="requiredAstrix">*</span></label>
                                                    <div class="col-sm-3">
                                                        <input type="text" class="form-control required validInput" id="ip-range-end-address-input-{{../../../executionFlow.id}}" name="ip-range-end-address-input-{{../../../executionFlow.id}}" autofocus="autofocus" value="" onchange="onEndIPChange(this);">
                                                    </div>
                                                </div>
                                            {{else}}
                                                <div class="form-group" style="display:none" id="specific-ip-block-{{../../../executionFlow.id}}">
                                                    <label class="control-label col-sm-3" for="request-count"><%=i18n.localize("IP Address:")%><span class="requiredAstrix">*</span></label>
                                                    <div class="col-sm-9">
                                                        <input type="text" class="form-control required validInput" id="specific-ip-address-input-{{../../../executionFlow.id}}" name="specific-ip-address-input-{{../../../executionFlow.id}}" autofocus="autofocus" value="" onchange="onIPChange(this);">
                                                    </div>
                                                </div>

