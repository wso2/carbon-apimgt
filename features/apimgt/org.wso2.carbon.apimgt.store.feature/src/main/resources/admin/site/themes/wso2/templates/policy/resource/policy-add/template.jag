                                                    <div class="form-group">
                                                        <label class="control-label col-sm-3" for="request-count">Time:
                                                            <span class="requiredAstrix">*</span>
                                                            <a class="help_popup" help_data="name_help" data-trigger="hover" data-placement="bottom" data-original-title="" title=""><i class="fa fa-question-circle" data-original-title="" title=""></i></a>
                                                            <p id="name_help" class="hide">Time Configuration.</p>
                                                        </label>
                                                        <div class="col-sm-6">
                                                            <input type="text" class="form-control" id="execution-flow-request-count-bandwidth-unit-time-{{executionFlow.id}}" name="execution-flow-unit-time-count-{{executionFlow.id}}" autofocus="autofocus" value="{{executionFlow.quotaPolicy.limit.unitTime}}">
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <select class="form-control select required valid" id="execution-flow-request-count-unit-{{executionFlow.id}}" name="execution-flow-request-count-unit-{{executionFlow.id}}">
                                                                <option value="min"  {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "min"}}selected{{else}}{{#if_eq executionFlow.quotaPolicy.limit.timeUnit ""}}selected{{/if_eq}}{{/if_eq}}>Minute(s)</option>
                                                                <option value="hour" {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "hour"}}selected{{/if_eq}}>Hour(s)</option>
                                                                <option value="day" {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "day"}}selected{{/if_eq}}>Day(s)</option>
                                                                <option value="week" {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "week"}}selected{{/if_eq}}>Week(s)</option>
                                                                <option value="month" {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "month"}}selected{{/if_eq}}>Month(s)</option>
                                                                <option value="year" {{#if_eq executionFlow.quotaPolicy.limit.timeUnit "year"}}selected{{/if_eq}}>Year(s)</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



</script>

