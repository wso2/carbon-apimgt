                         <div class="control-group">
                            <label class="control-label" for="techOwnerMail"><%=i18n.localize("techOwnerMail")%>:</label>
                            <div class="controls">
                                 <input type="text" class="input-xlarge email" id="techOwnerMail" name="techOwnerMail"/>
                            </div>
                        </div>
                        
                    </div>
                </fieldset>
                
                
                
                <fieldset>
